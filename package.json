  "description": "Multilingual WhatsApp healthcare bot using Supabase and Gemini 2.0 Flash",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "test": "jest",
    "test:watch": "jest --watch",
