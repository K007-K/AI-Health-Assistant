    "compression": "^1.7.4",
    "winston": "^3.11.0",
    "joi": "^17.11.0",
    "moment": "^2.29.4",
    "uuid": "^9.0.1",
    "node-cron": "^3.0.3",
    "mongoose": "^8.0.0",
    "xml2js": "^0.6.2"
